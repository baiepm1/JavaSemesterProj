import java.util.Arrays;

public class solution {
	 public String solution(int card1, int card2, int card3, int card4)	//call this to fill 
	    {
		 String solve = null;
		 
		 int[] cards = {card1, card2, card3, card4};
		 Arrays.sort(cards);
		 
		 for(int i = 0; i < cards.length; i++) {
			 solve += cards[i];
		 }
		 
		 return solve;
	    }
}
